{
  "name": "@earthbnb/controller",
  "version": "0.1.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "gen:schema": "apollo schema:download --endpoint=http://localhost:4000",
    "gen:types": "apollo codegen:generate --queries=./src/**/*.tsx --schema=./schema.json --target=typescript --outputFlat  ./src/genTypes.ts",
    "generate": "yarn gen:schema && yarn gen:types",
    "build": "rimraf ./dist && yarn generate && tsc"
  },
  "dependencies": {
    "graphql-tag": "^2.9.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@types/node": "^10.5.3",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "apollo": "^1.5.0",
    "apollo-codegen": "^0.20.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^2.9.2"
  }
}
